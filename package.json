{
  "name": "ocaml",
  "version": "4.2.3000+buckle-master-esy",
  "description": "OCaml Compiler as an npm Package",
  "esy": {
    "build": [
      "./configure -no-cfi -prefix #{self.install}",
      "make -j1 world.opt"
    ],
    "install": [
      "make install"
    ],
    "buildsInSource": true,
    "exportedEnv": {
      "OCAMLLIB": {
        "val": "#{self.install / 'lib' / 'ocaml'}",
        "scope": "global"
      },
      "CAML_LD_LIBRARY_PATH": {
        "val": "#{self.install / 'lib' / 'ocaml' / 'stublibs' : self.install / 'lib' / 'ocaml' : $CAML_LD_LIBRARY_PATH}",
        "scope": "global"
      },
      "OCAML_TOPLEVEL_PATH": {
        "val": "#{self.install / 'lib' / 'ocaml'}",
        "scope": "global"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/esy-ocaml/ocaml.git"
  },
  "keywords": [
    "ocaml",
    "flow",
    "opam"
  ],
  "author": "",
  "license": "QPL - See LICENSE at https://github.com/ocaml/ocaml",
  "bugs": {
    "url": "https://github.com/esy-ocaml/ocaml/issues"
  },
  "homepage": "https://github.com/esy-ocaml/ocaml#readme"
}
